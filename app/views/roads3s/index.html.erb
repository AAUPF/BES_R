<p id="notice"><%= notice %></p>

<h1>Roads3s</h1>

<table>
  <thead>
    <tr>
      <th>State</th>
      <th>Nh</th>
      <th>Sh</th>
      <th>Other</th>
      <th>Total</th>
      <th>Indicator</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @roads3s.each do |roads3| %>
      <tr>
        <td><%= roads3.State %></td>
        <td><%= roads3.NH %></td>
        <td><%= roads3.SH %></td>
        <td><%= roads3.Other %></td>
        <td><%= roads3.Total %></td>
        <td><%= roads3.Indicator %></td>
       
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Roads3', new_roads3_path %>