  <tbody>
    <% @state_public_sector2s.each do |state_public_sector2| %>
      <tr>
        <td><%= state_public_sector2.Type_of_Company_or_corporation %></td>
        <td><%= state_public_sector2.Government_Companies_Capital %></td>
        <td><%= state_public_sector2.Government_Companies_Long_Term_Loans %></td>
        <td><%= state_public_sector2.Total_Government_Companies %></td>
        <td><%= state_public_sector2.Statutory_Corporations_Capital %></td>
        <td><%= state_public_sector2.Statutory_Corporations_Long_Term_Loans %></td>
        <td><%= state_public_sector2.Total_Statutory_Corporations %></td>
        <td><%= state_public_sector2.Grand_Total %></td>
       
      </tr>
    <% end %>
  </tbody>
</table>

