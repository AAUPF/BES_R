<p id="notice"><%= notice %></p>

<h1>Agricultural Inputs4s</h1>

<table>
  <thead>
    <tr>
      <th>Districts</th>
      <th>Urea</th>
      <th>Dap</th>
      <th>Ssp</th>
      <th>Mop</th>
      <th>Ammonium sulphate</th>
      <th>Complex</th>
      <th>Total</th>
      <th>N</th>
      <th>P</th>
      <th>K</th>
      <th>Total npk</th>
      <th>Grand total</th>
      <th>Year</th>
      <th>Percentage urea</th>
      <th>Percentage dap</th>
      <th>Percentage ssp</th>
      <th>Percentage mop</th>
      <th>Percentage ammonium sulphate</th>
      <th>Percentage complex</th>
      <th>Percentage total npk</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @agricultural_inputs4s.each do |agricultural_inputs4| %>
      <tr>
        <td><%= agricultural_inputs4.Districts %></td>
        <td><%= agricultural_inputs4.Urea %></td>
        <td><%= agricultural_inputs4.DAP %></td>
        <td><%= agricultural_inputs4.SSP %></td>
        <td><%= agricultural_inputs4.MOP %></td>
        <td><%= agricultural_inputs4.Ammonium_Sulphate %></td>
        <td><%= agricultural_inputs4.Complex %></td>
        <td><%= agricultural_inputs4.Total %></td>
        <td><%= agricultural_inputs4.N %></td>
        <td><%= agricultural_inputs4.P %></td>
        <td><%= agricultural_inputs4.K %></td>
        <td><%= agricultural_inputs4.Total_NPK %></td>
        <td><%= agricultural_inputs4.Grand_Total %></td>
        <td><%= agricultural_inputs4.Year %></td>
        <td><%= agricultural_inputs4.Percentage_Urea %></td>
        <td><%= agricultural_inputs4.Percentage_DAP %></td>
        <td><%= agricultural_inputs4.Percentage_SSP %></td>
        <td><%= agricultural_inputs4.Percentage_MOP %></td>
        <td><%= agricultural_inputs4.Percentage_Ammonium_Sulphate %></td>
        <td><%= agricultural_inputs4.Percentage_Complex %></td>
        <td><%= agricultural_inputs4.Percentage_Total_NPK %></td>
       
      </tr>
    <% end %>
  </tbody>
</table>

