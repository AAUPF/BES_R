<p id="notice"><%= notice %></p>

<h1>Irrigation2s</h1>

<table>
  <thead>
    <tr>
      <th>Name of scheme</th>
      <th>Created irrigation potential</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @irrigation2s.each do |irrigation2| %>
      <tr>
        <td><%= irrigation2.Name_of_Scheme %></td>
        <td><%= irrigation2.Created_Irrigation_Potential %></td>
        <td><%= link_to 'Show', irrigation2 %></td>
        <td><%= link_to 'Edit', edit_irrigation2_path(irrigation2) %></td>
        <td><%= link_to 'Destroy', irrigation2, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Irrigation2', new_irrigation2_path %>
