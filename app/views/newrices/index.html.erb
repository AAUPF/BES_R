<p id="notice"><%= notice %></p>

<h1>Newrices</h1>

<table>
  <thead>
    <tr>
      <th>Districts</th>
      <th>Area</th>
      <th>Production</th>
      <th>Productivity</th>
      <th>Year</th>
      <th>Percentage area</th>
      <th>Percentage production</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @newrices.each do |newrice| %>
      <tr>
        <td><%= newrice.Districts %></td>
        <td><%= newrice.Area %></td>
        <td><%= newrice.Production %></td>
        <td><%= newrice.Productivity %></td>
        <td><%= newrice.Year %></td>
        <td><%= newrice.Percentage_Area %></td>
        <td><%= newrice.Percentage_Production %></td>
       
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Newrice', new_newrice_path %>