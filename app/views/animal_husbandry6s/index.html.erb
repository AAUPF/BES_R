        <td><%= animal_husbandry6.Percentage_Crossbred_Cow %></td>
        <td><%= animal_husbandry6.Percentage_Local_Cow %></td>
        <td><%= animal_husbandry6.Percentage_Total_Cow %></td>
        <td><%= animal_husbandry6.Percentage_Buffalo %></td>
        <td><%= animal_husbandry6.Percentage_Total_Cow_Buffalo %></td>
        <td><%= animal_husbandry6.Percentage_Goat %></td>
        <td><%= animal_husbandry6.Percentage_Total_Production %></td>
