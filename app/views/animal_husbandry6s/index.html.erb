  </thead>

  <tbody>
    <% @animal_husbandry6s.each do |animal_husbandry6| %>
      <tr>
        <td><%= animal_husbandry6.Districts %></td>
        <td><%= animal_husbandry6.Crossbred_Cow %></td>
        <td><%= animal_husbandry6.Local_Cow %></td>
