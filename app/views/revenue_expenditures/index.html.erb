  <tbody>
    <% @revenue_expenditures.each do |revenue_expenditure| %>
      <tr>
        <td><%= revenue_expenditure.Sector %></td>
        <td><%= revenue_expenditure.2012-13 %></td>
        <td><%= revenue_expenditure.2013-14 %></td>
        <td><%= revenue_expenditure.2014-15 %></td>
        <td><%= revenue_expenditure.2015-16 %></td>
        <td><%= revenue_expenditure.2016-17 %></td>
        <td><%= revenue_expenditure.2017-18_BE %></td>
        <td><%= revenue_expenditure.2012-17 %></td>
       
      </tr>
    <% end %>
  </tbody>
</table>

