<p id="notice"><%= notice %></p>

<h1>Agricultural Inputs3s</h1>

<table>
  <thead>
    <tr>
      <th>Year</th>
      <th>Tractor</th>
      <th>Combine harvestors</th>
      <th>Zero tillage</th>
      <th>Pumpsets</th>
      <th>Power tiller</th>
      <th>Manually operated tools implements</th>
      <th>Threshers</th>
      <th>Total</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @agricultural_inputs3s.each do |agricultural_inputs3| %>
      <tr>
        <td><%= agricultural_inputs3.Year %></td>
        <td><%= agricultural_inputs3.Tractor %></td>
        <td><%= agricultural_inputs3.Combine_Harvestors %></td>
        <td><%= agricultural_inputs3.Zero_Tillage %></td>
        <td><%= agricultural_inputs3.Pumpsets %></td>
        <td><%= agricultural_inputs3.Power_Tiller %></td>
        <td><%= agricultural_inputs3.Manually_operated_tools_implements %></td>
        <td><%= agricultural_inputs3.Threshers %></td>
        <td><%= agricultural_inputs3.Total %></td>
        <td><%= link_to 'Show', agricultural_inputs3 %></td>
        <td><%= link_to 'Edit', edit_agricultural_inputs3_path(agricultural_inputs3) %></td>
        <td><%= link_to 'Destroy', agricultural_inputs3, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

