<p id="notice"><%= notice %></p>

<h1>Ict4s</h1>

<table>
  <thead>
    <tr>
      <th>State</th>
      <th>Rural</th>
      <th>Urban</th>
      <th>Total</th>
      <th>Rural percentage</th>
      <th>Urban percentage</th>
      <th>Share in all india</th>
      <th>Indicator</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @ict4s.each do |ict4| %>
      <tr>
        <td><%= ict4.State %></td>
        <td><%= ict4.Rural %></td>
        <td><%= ict4.Urban %></td>
        <td><%= ict4.Total %></td>
        <td><%= ict4.Rural_Percentage %></td>
        <td><%= ict4.Urban_Percentage %></td>
        <td><%= ict4.Share_in_all_India %></td>
        <td><%= ict4.Indicator %></td>
       
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Ict4', new_ict4_path %>