<p id="notice"><%= notice %></p>

<h1>Health11s</h1>

<table>
  <thead>
    <tr>
      <th>Diseases</th>
      <th>Number of patients</th>
      <th>Year</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @health11s.each do |health11| %>
      <tr>
        <td><%= health11.Diseases %></td>
        <td><%= health11.Number_of_Patients %></td>
        <td><%= health11.Year %></td>
        <td><%= link_to 'Show', health11 %></td>
        <td><%= link_to 'Edit', edit_health11_path(health11) %></td>
        <td><%= link_to 'Destroy', health11, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Health11', new_health11_path %>
