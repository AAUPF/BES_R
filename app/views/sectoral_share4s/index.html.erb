<p id="notice"><%= notice %></p>

<h1>Sectoral Share4s</h1>

<table>
  <thead>
    <tr>
      <th>Year</th>
      <th>Target</th>
      <th>Achievement</th>
      <th>Achievement percentage</th>
      <th>Indicator</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @sectoral_share4s.each do |sectoral_share4| %>
      <tr>
        <td><%= sectoral_share4.Year %></td>
        <td><%= sectoral_share4.Target %></td>
        <td><%= sectoral_share4.Achievement %></td>
        <td><%= sectoral_share4.Achievement_Percentage %></td>
        <td><%= sectoral_share4.Indicator %></td>
       
      </tr>
    <% end %>
  </tbody>
</table>

